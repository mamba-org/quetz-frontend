{
  "name": "@quetz-frontend/app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "yarn run clean && webpack",
    "build:dev": "yarn run build",
    "build:prod": "webpack --config webpack.prod.config.js",
    "build:prod:minimize": "webpack --config webpack.prod.minimize.config.js",
    "build:prod:release": "webpack --config webpack.prod.release.config.js",
    "build:prod:stats": "webpack --profile --config webpack.prod.minimize.config.js --json > stats.json",
    "build:stats": "webpack --profile --json > stats.json",
    "clean": "rimraf build",
    "prepublishOnly": "npm run build",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "@jupyterlab/apputils-extension": "^3.0.0",
    "@jupyterlab/theme-dark-extension": "^3.0.0",
    "@jupyterlab/theme-light-extension": "^3.0.0",
    "@quetz-frontend/application-extension": "^0.1.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.6",
    "@jupyterlab/buildutils": "^3.0.4",
    "chokidar": "^3.4.0",
    "css-loader": "^5.0.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "~6.0.0",
    "fs-extra": "^9.0.1",
    "glob": "~7.1.6",
    "handlebars": "^4.5.3",
    "html-loader": "~1.3.0",
    "html-webpack-plugin": "~5.3.1",
    "mini-css-extract-plugin": "~1.3.2",
    "raw-loader": "~4.0.0",
    "rimraf": "~3.0.0",
    "sort-package-json": "~1.44.0",
    "source-map-loader": "~1.0.2",
    "style-loader": "~2.0.0",
    "svg-url-loader": "~6.0.0",
    "terser-webpack-plugin": "^4.1.0",
    "url-loader": "~4.1.0",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3",
    "whatwg-fetch": "^3.0.0",
    "worker-loader": "^3.0.2",
    "yarn-deduplicate": "^2.1.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "jupyterlab": {
    "name": "Quetz-Frontend",
    "version": "0.1.0",
    "extensions": [
      "@jupyterlab/apputils-extension",
      "@jupyterlab/theme-dark-extension",
      "@jupyterlab/theme-light-extension",
      "@quetz-frontend/application-extension"
    ]
  }
}
