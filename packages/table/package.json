    "@jupyterlab/services": "^6.2.0",
    "@quetz-frontend/apputils": "^0.3.0",
    "clsx": "^1.1.1",
    "react": "^17.0.1",
    "react-table": "^7.6.3",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-table": "^7.0.27",
    "rimraf": "~3.0.2",
    "typescript": "~4.1.3"
