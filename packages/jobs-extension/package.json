{
  "name": "@quetz-frontend/jobs-extension",
  "version": "0.3.0",
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/*.css",
    "style/index.js"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "clean:all": "yarn run clean && rimraf node_modules",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.2.0",
    "@jupyterlab/apputils": "^3.2.0",
    "@jupyterlab/coreutils": "^5.2.0",
    "@jupyterlab/services": "^6.2.0",
    "@quetz-frontend/application": "^0.3.0",
    "@quetz-frontend/apputils": "^0.3.0",
    "@quetz-frontend/menu": "^0.3.0",
    "@quetz-frontend/table": "^0.3.0",
    "react": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-table": "^7.6.3",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-table": "^7.0.27",
    "rimraf": "~3.0.2",
    "typescript": "~4.1.3"
  },
  "quetz": {
    "coreDependency": true
  },
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  }
}
