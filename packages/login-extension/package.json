{
  "name": "@quetz-frontend/login-extension",
  "version": "0.2.0",
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/*.css",
    "style/index.js",
    "style/img/*.{jpg,png,svg}"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "clean:all": "yarn run clean && rimraf node_modules",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@quetz-frontend/application": "^0.2.0",
    "@quetz-frontend/menu": "^0.2.0",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "rimraf": "~3.0.2",
    "typescript": "~4.1.3"
  },
  "quetz": {
    "coreDependency": true
  },
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  }
}
