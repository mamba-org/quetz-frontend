{
  "name": "@quetz-frontend/application",
  "version": "0.3.0",
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/*.css",
    "style/index.js"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "clean:all": "yarn run clean && rimraf node_modules",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.2.0",
    "@jupyterlab/apputils": "^3.2.0",
    "@jupyterlab/coreutils": "^5.2.0",
    "@jupyterlab/docregistry": "^3.2.0",
    "@jupyterlab/services": "^6.2.0",
    "@jupyterlab/ui-components": "^3.2.0",
    "@lumino/algorithm": "^1.3.3",
    "@lumino/application": "^1.25.0",
    "@lumino/messaging": "^1.4.3",
    "@lumino/widgets": "^1.19.0"
  },
  "devDependencies": {
    "rimraf": "~3.0.2",
    "typescript": "~4.1.3"
  },
  "quetz": {
    "coreDependency": true
  },
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  }
}
